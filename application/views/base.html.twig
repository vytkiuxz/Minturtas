<div class="property-filter widget">
    <div class="content">
        <form method="get" action="{{ form_url }}" name="search_form" class="search_form">
            <div class="location control-group">
                <label class="control-label">
                    Miestas
                </label>

                <div class="controls">
                    <select name="filter_location">
						<option value="">-</option>
						{% for location in locations %}
						<option value="{{ location.id }}" {% if  parameters.location_id is defined and parameters.location_id == location.id %} selected="selected" {% endif %}>{{ location.name }}</option>
						{% endfor %}
                    </select>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="type control-group">
                <label class="control-label">
                    Tipas
                </label>

                <div class="controls">
                    <select name="filter_type">
                        <option value="">-</option>
                        {% for type in types %}
						<option value="{{ type.id }}" {% if  parameters.type_id is defined and parameters.type_id == type.id %} selected="selected" {% endif %}>{{ type.name }}</option>
						{% endfor %}
                    </select>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->


            <div class="rent control-group">
                <div class="controls">
                    <label class="checkbox">
                        <input type="checkbox" value="1" {% if parameters.rent is defined %} checked="checked" {% endif %} name="filter_rent"> Nuomoja
                    </label>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="sale control-group">
                <div class="controls">
                    <label class="checkbox">
                        <input type="checkbox" value="1" {% if parameters.sale is defined %} checked="checked" {% endif %} name="filter_sale"> Parduoda
                    </label>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="price-from control-group">
                <label class="control-label">
                    Kaina nuo
                </label>

                <div class="controls">
                    <select name="filter_price_from">
                        <option value="">-</option>
                        <option value="5000" {% if parameters.price_from is defined and parameters.price_from == 5000 %} selected="selected" {% endif %}>5,000 €</option>
                        <option value="20000" {% if parameters.price_from is defined and parameters.price_from == 20000 %} selected="selected" {% endif %}>20,000 €</option>
                        <option value="50000" {% if parameters.price_from is defined and parameters.price_from == 50000 %} selected="selected" {% endif %}>50,000 €</option>
                        <option value="100000" {% if parameters.price_from is defined and parameters.price_from == 100000 %} selected="selected" {% endif %}>100,000 €</option>
				        <option value="200000" {% if parameters.price_from is defined and parameters.price_from == 200000 %} selected="selected" {% endif %}>200,000 €</option>
                        <option value="500000" {% if parameters.price_from is defined and parameters.price_from == 500000 %} selected="selected" {% endif %}>500,000 €</option>
                    </select>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="price-to control-group">
                <label class="control-label">
                    Kaina iki
                </label>

                <div class="controls">
                    <select name="filter_price_to">
                        <option value="">-</option>
                        <option value="5000" {% if parameters.price_to is defined and parameters.price_to == 5000 %} selected="selected" {% endif %}>5,000 €</option>
                        <option value="20000" {% if parameters.price_to is defined and parameters.price_to == 20000 %} selected="selected" {% endif %}>20,000 €</option>
                        <option value="50000" {% if parameters.price_to is defined and parameters.price_to == 50000 %} selected="selected" {% endif %}>50,000 €</option>
                        <option value="100000" {% if parameters.price_to is defined and parameters.price_to == 100000 %} selected="selected" {% endif %}>100,000 €</option>
				        <option value="200000" {% if parameters.price_to is defined and parameters.price_to == 200000 %} selected="selected" {% endif %}>200,000 €</option>
                        <option value="500000" {% if parameters.price_to is defined and parameters.price_to == 500000 %} selected="selected" {% endif %}>500,000 €</option>
                    </select>
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="area-from control-group">
                <label class="control-label">
                    Plotas nuo
                </label>

                <div class="controls">
                    <input type="text" value="{{ parameters.area_from }}" name="filter_area_from">
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="area-to control-group">
                <label class="control-label">
                    Plotas iki
                </label>

                <div class="controls">
                    <input type="text" value="{{ parameters.area_to }}" name="filter_area_to">
                </div>
                <!-- /.controls -->
            </div>
            <!-- /.control-group -->

            <div class="form-actions">
                <button class="btn btn-primary btn-large">Ieškoti</button>
            </div>
			
			<input class="sort_dir" type="hidden" name="sort_dir"/>
			<input class="sort_by" type="hidden" name="sort_by"/>
            <!-- /.form-actions -->
        </form>
    </div>
